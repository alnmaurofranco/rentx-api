{
  "name": "rentx-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && sleep 1 && babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "dotenv -e .env.production node dist/infra/http/server.js",
    "start:prod": "yarn build && yarn start",
    "start:dev": "NODE_ENV=development tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --clear src/infra/http/server.ts",
    "dev:debug": "tsnd -r tsconfig-paths/register --inspect --respawn --transpile-only --ignore-watch node_modules --clear src/infra/http/server.ts",
    "typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "db:migrations": "yarn typeorm migration:create -n",
    "db:migrate": "yarn typeorm migration:run",
    "db:migrate:deploy": "dotenv -e .env.production yarn typeorm migration:run",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "seed:admin": "tsnd -r tsconfig-paths/register src/infra/typeorm/seeds/admin.ts",
    "create:pm2:app": "NODE_ENV=production pm2 start dist/infra/http/server.js --name rentx-api",
    "stop:app": "pm2 stop rentx-api"
  },
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1084.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "csv-parse": "^4.16.3",
    "dayjs": "^1.10.7",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.0",
    "pg": "^8.7.1",
    "rate-limiter-flexible": "^2.3.6",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.1",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.2.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
